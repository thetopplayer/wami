			<TextView
					android:id="@+id/profile_name_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/line"
					android:layout_marginTop="15dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Profile Name"/>

			<TextView
					android:id="@+id/profile_name"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/profile_name_title"
					android:layout_marginTop="3dp"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"
					android:singleLine="true"/>

			<TextView
					android:id="@+id/description_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/profile_name"
					android:layout_marginTop="15dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Profile Description"/>

			<TextView
					android:id="@+id/description"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/description_title"
					android:layout_marginTop="3dp"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"
					android:singleLine="true"/>


			<TextView
					android:id="@+id/contact_name_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/description"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Contact Name"/>

			<TextView
					android:id="@+id/contact_name"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/contact_name_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/email_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/contact_name"
					android:paddingTop="10dp"
					android:textColor="#FF7DA9A8"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Email Address"/>

			<TextView
					android:id="@+id/email"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/email_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#FF7DA9A8"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/profile_type_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/email"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Profile Type"/>

			<TextView
					android:id="@+id/profile_type"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/profile_type_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/tags_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/profile_type"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Tags"/>

			<TextView
					android:id="@+id/tags"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/tags_title"
					android:layout_marginTop="3dp"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/groups_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/tags"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="My Group(s)"/>

			<TextView
					android:id="@+id/groups"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/groups_title"
					android:layout_marginTop="3dp"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/street_address_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
