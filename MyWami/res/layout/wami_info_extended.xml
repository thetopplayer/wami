					android:textSize="15sp"
					android:text="Street Address"/>

			<TextView
					android:id="@+id/street_address"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/street_address_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/city_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/street_address"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="City"/>

			<TextView
					android:id="@+id/city"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/city_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/state_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/city"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="State"/>

			<TextView
					android:id="@+id/state"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/state_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/zipcode_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/state"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Zip Code"/>

			<TextView
					android:id="@+id/zipcode"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/zipcode_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/country_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/zipcode"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Country"/>

			<TextView
					android:id="@+id/country"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/country_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/telephone_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/country"
					android:paddingTop="10dp"
					android:textColor="#FF7DA9A8"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Telephone"/>

			<TextView
					android:id="@+id/telephone"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/telephone_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#FF7DA9A8"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/create_date_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/telephone"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Profile Create Date"/>

			<TextView
					android:id="@+id/create_date"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/create_date_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/searchable_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/create_date"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Profile Searchable?"/>

			<TextView
					android:id="@+id/searchable"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/searchable_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>

			<TextView
					android:id="@+id/active_ind_title"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/searchable"
					android:paddingTop="10dp"
					android:textColor="#000000"
					android:textStyle="bold"
					android:textSize="15sp"
					android:text="Profile Status"/>

			<TextView
					android:id="@+id/active_ind"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:layout_below="@+id/active_ind_title"
					android:layout_marginTop="3dp"
					android:singleLine="true"
					android:textColor="#ff5c5c5c"
					android:textSize="17sp"
					android:padding="5dp"
					android:background="@drawable/textfield_background"/>
		</RelativeLayout>
	</ScrollView>


